{
    "config_version": "2.0",
    "preset": "intermediate",
    
    "_comment": "Balanced GPU mining configuration for PRIME channel",
    "_description": "Good balance between performance and power efficiency",
    
    "wallet_ip": "primepool.nexus.io",
    "port": 50000,
    "mining_mode": "PRIME",
    
    "pool": {
        "address": "<INSERT_YOUR_NXS_ADDRESS>",
        "display_name": "<YOUR_DISPLAY_NAME>"
    },
    
    "power_profile": "balanced",
    "power_limit_percent": 90,
    "target_hashrate": 0,
    
    "console_logging": true,
    "file_logging": true,
    "log_file": "miner.log",
    "log_level": 2,
    "stats_interval": 15,
    
    "workers": [
        {
            "id": "gpu0",
            "hardware": "gpu",
            "gpu": {
                "device": 0,
                "power_limit_percent": 90,
                "power_profile": "balanced",
                "target_hashrate": 0,
                "target_power": 0,
                "fan_speed": 0,
                "core_clock_offset": 0,
                "memory_clock_offset": 0
            }
        }
    ],
    
    "enable_block_signing": false
}
